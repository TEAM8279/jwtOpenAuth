  {
    "type": "post",
    "url": "/login/key",
    "title": "Request for validate the double check key",
    "name": "key_validation",
    "group": "login",
    "header": {
      "examples": [
        {
          "title": "Header-Example:",
          "content": "{\n  \"Authorization\": \"Bearer [token]\"\n}",
          "type": "json"
        }
      ]
    },
    "parameter": {
      "fields": {
        "Parameter": [
          {
            "group": "Parameter",
            "type": "number",
            "optional": false,
            "field": "key",
            "description": "<p>generated by the totp app</p>"
          }
        ]
      }
    },
    "success": {
      "examples": [
        {
          "title": "Sucess-Response:",
          "content": "HTTP/1.1 200 OK\n{\n \"id\": 1,\n \"name\": \"John Doe\",\n \"mail\": \"johndoe@exemple.com\",\n \"token\": \"eyJ0eXAiOOiJIUzI1NiJ9.eyJpZCI6IjJtYWlUsiaWF0IjoxNTgxNDE4NTgyfQ.4ip40sLnhgeRDfkhjHVo\",\n \"double-check\": true\n}",
          "type": "json"
        }
      ]
    },
    "error": {
      "fields": {
        "Error 4xx": [
          {
            "group": "Error 4xx",
            "optional": false,
            "field": "BearerNotSet",
            "description": "<p>the bearer is not in the body</p>"
          },
          {
            "group": "Error 4xx",
            "optional": false,
            "field": "InvalidToken",
            "description": "<p>the token you try to use is invalid or expired</p>"
          },
          {
            "group": "Error 4xx",
            "optional": false,
            "field": "MissingKey",
            "description": "<p>you don't have set the key param in the body</p>"
          },
          {
            "group": "Error 4xx",
            "optional": false,
            "field": "UnsetSecret",
            "description": "<p>your secret isn't generated</p>"
          },
          {
            "group": "Error 4xx",
            "optional": false,
            "field": "UnvalidateSecret",
            "description": "<p>your secret isn't validate</p>"
          },
          {
            "group": "Error 4xx",
            "optional": false,
            "field": "WrongKey",
            "description": "<p>the key you try to use is deprecated</p>"
          }
        ]
      }
    },
    "version": "0.0.0",
    "filename": "app/routes.php",
    "groupTitle": "login"
  },
