      "fields": {
        "Parameter": [
          {
            "group": "Parameter",
            "type": "string",
            "optional": false,
            "field": "name",
            "description": "<p>name of the new user</p>"
          },
          {
            "group": "Parameter",
            "type": "string",
            "optional": false,
            "field": "mail",
            "description": "<p>mail of the new user</p>"
          },
          {
            "group": "Parameter",
            "type": "string",
            "optional": false,
            "field": "password",
            "description": "<p>password of the new user</p>"
          }
        ]
      }
    },
    "success": {
      "examples": [
        {
          "title": "Sucess-Response:",
          "content": "HTTP/1.1 201 Created\n{\n \"name\": \"John Doe\",\n \"mail\": \"johndoe@exemple.com\",\n}",
          "type": "json"
        }
      ]
    },
    "error": {
      "fields": {
        "Error 4xx": [
          {
            "group": "Error 4xx",
            "optional": false,
